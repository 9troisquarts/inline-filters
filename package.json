{
  "name": "@9troisquarts/inline-filters",
  "private": false,
  "version": "1.0.4",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/inline-filters.umd.cjs",
  "module": "./dist/inline-filters.js",
  "exports": {
    ".": {
      "import": "./dist/inline-filters.js",
      "require": "./dist/inline-filters.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "release": "npx lint-staged && standard-version && npm run build && npm publish",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "@ant-design/icons": "<5",
    "ahooks": "<4",
    "antd": "<5",
    "react-inlinesvg": "<5"
  },
  "lint-staged": {
    "lib/*.{js,jsx,less,md,json}": [
      "prettier --write --ignore-unknown"
    ],
    "lib/*.ts?(x)": [
      "prettier --parser=typescript --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "@ant-design/icons": "<5",
    "@faker-js/faker": "^9.0.0",
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-interactions": "^7.6.3",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/addon-onboarding": "^1.0.9",
    "@storybook/blocks": "^7.6.3",
    "@storybook/react": "^7.6.3",
    "@storybook/react-vite": "^7.6.3",
    "@storybook/test": "^7.6.3",
    "@types/react": "<19",
    "@types/react-dom": "<19",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "ahooks": "^3",
    "antd": "<5",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.1",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "react": "<19",
    "react-dom": "<19",
    "react-inlinesvg": "^4",
    "standard-version": "^9.5.0",
    "storybook": "^7.6.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.8",
    "vite-plugin-ruby": "^5.0.0"
  },
  "dependencies": {},
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
